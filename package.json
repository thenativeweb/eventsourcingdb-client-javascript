{
  "name": "eventsourcingdb",
  "version": "0.2.0",
  "description": "eventsourcingdb is the JavaScript client for EventSourcingDB.",
  "contributors": [
    {
      "name": "Golo Roden",
      "email": "golo.roden@thenativeweb.io"
    },
    {
      "name": "Noah Hummel",
      "email": "noah.hummel@thenativeweb.io"
    },
    {
      "name": "Alexander Kampf",
      "email": "alexander.kampf@thenativeweb.io"
    }
  ],
  "private": true,
  "main": "build/lib/index.js",
  "types": "build/lib/index.d.ts",
  "dependencies": {
    "axios": "1.2.3",
    "http-status-codes": "2.2.0",
    "stream-to-async-iterator": "1.0.0"
  },
  "devDependencies": {
    "@types/mocha": "10.0.1",
    "@types/shelljs": "0.8.11",
    "assertthat": "6.5.2",
    "mocha": "10.2.0",
    "rome": "11.0.0",
    "shelljs": "0.8.5",
    "ts-node": "10.9.1",
    "typescript": "4.9.4"
  },
  "scripts": {
    "analyze": "npx rome ci ./**/*",
    "build": "npx tsc",
    "format": "npx rome format --write ./**/*",
    "qa": "npm run analyze && npm run test",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "npx mocha --bail --require ts-node/register --recursive --ui tdd ./test/unit/**/*.ts",
    "test:integration": "npx mocha --bail --require ts-node/register --recursive --ui tdd ./test/integration/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/thenativeweb/eventsourcingdb-client-javascript.git"
  }
}
